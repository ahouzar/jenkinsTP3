    agent any
    stages {
        stage('Build') {
            steps {
                echo "Etape de build"
                sh 'python22 --version'
            }
        }
        stage('Test') {
            steps {
                echo "Etape de test"
            }
        }
        stage('Deploy') {
            steps {
                echo "etape de deployement"
            }
        }
    }
}

